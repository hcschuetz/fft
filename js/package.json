{
  "name": "fft",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "test": "node dst/test.js",
    "benchmark": "node dst/benchmark.js",
    "watch:tsc": "tsc --watch",
    "watch:test": "nodemon -e js,txt --delay 1.0 dst/test/test.js",
    "watch:benchmark": "nodemon -e js,txt --delay 1.0 dst/test/benchmark.js",
    "opt": "node --print-bytecode --print-opt-code ./dst/test/benchmark.js > bytecode.txt",
    "clean": "rimraf dst bytecode.txt"
  },
  "devDependencies": {
    "@types/node": "^17.0.19",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  }
}
